{
  "name": "monorepo-simple",
  "description": "Monorepo simple",
  "workspaces": {
    "packages": [
      "libraries/types",
      "libraries/*",
      "services/*"
    ]
  },
  "private": true,
  "scripts": {
    "build:libraries": "yarn constraints && yarn workspaces foreach --recursive --parallel -vv --topological-dev --from '@libraries/*' run build"
  },
  "devDependencies": {
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.8.2",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "@yarnpkg/types": "^4.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.10.1",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "nestjs-redoc/@nestjs/core": "^9.0.0",
    "nestjs-redoc/@nestjs/common": "^9.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2"
  },
  "engines": {
    "npm": ">=7"
  },
  "lint-staged": {
    "*.(ts|js|tsx)": [
      "yarn prettier --write",
      "yarn eslint --fix",
      "yarn lint:betterer"
    ],
    "*.json": [
      "yarn prettier --write"
    ]
  },
  "version": "1.22.22",
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "@types/jsdom": "^21.1.7"
  }
}
